                password: {
                    required: true,
                    minlength: 5
                }
            },
            message: {
                name: {
                    required: 'Please enter username',
                    minlength: 'Should be minimum 3 characters'
                },
                password: {
                    required: 'Please enter the password',
                    minlength: 'Should be minimum 5 characters'
                },
                email: {
                    required: 'Enter the email id'
                }
            },

            submitHandler: function (form) {
                form.submit();
            }
        });
    </script>



    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script>
        function validateForm() {

            let emailError = true;
            var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
            function validateEmail() {
                let emailval = $('#email').val()
                if (emailval == "") {
                    $('#email').css('border-color', '#f30000')
                    $('#email').attr("placeholder", "Enter the email ID")
                    emailError = false;
                    return false
                }
                if (!emailReg.test(emailval)) {
                    $('#email').css('border-color', '#f30000')
                    $('#email').attr("placeholder", "Enter valid email id")
                    emailError = false
                    return false
                }
            }

            let passwordError = true
            function validatePassword() {
                let passwordval = $('#password').val()
                if (passwordval == "") {
                    $('#password').css('border-color', '#f30000')
                    $('#password').attr("placeholder", "Enter password")
                    passwordError = false
                    return false
                }

            }

            let usernameError = true
            function validateName() {
                let nameval = $('#name').val()
                if (nameval == "") {
                    $('#name').css('border-color', '#f30000')
                    $('#name').attr("placeholder", "Enter userName")
                    usernameError = false
                    return false
                }
                if(nameval.length<3){
                    $('#name').css('border-color', '#f30000')
                    $('#name').attr("placeholder", "Minimum length 3")
                    usernameError = false
                    return false 
                }

            }

            validatePassword()
            validateEmail()
            validateName()

            if (emailError === true && passwordError === true&& usernameError===true) {
                return true
            } else {
                return false
            }

        }
    </script>



