    <script src="/user/assets/vendor/imagesloaded/imagesloaded.pkgd.min.js"></script>
    <script src="/user/assets/vendor/zoom/jquery.zoom.js"></script>
    <script src="/user/assets/vendor/magnific-popup/jquery.magnific-popup.min.js"></script>


