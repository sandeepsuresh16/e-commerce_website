    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script>
        function validateForm(){
            let mobileError = true;

        function validateMobile() {

            let regnum = new RegExp("^[0-9 ]+$");

            let mobileValue = $('#phone').val();

            if (mobileValue.length == '') {

                $('#phone').css('border-color', '#c70d0d')
                $('#phone').attr("placeholder", "Enter Number")

                mobileError = false;
                return false;
            } else if (!regnum.test(mobileValue)) {

                $('#phone').css('border-color', '#c70d0d')
                $("#mobilewarn").text('Enter Numbers Only')

                mobileError = false;
            } else if (mobileValue.length != 10) {

                $('#phone').css('border-color', '#c70d0d')
                $("#mobilewarn").text('Mobile number must Contain 10 Digit')

                mobileError = false;
            }

        }
        validateMobile()
        if(mobileError===true){
            return true
        }else{
            return false
        }

        }


        function validateForm1(){
            let otpError = true;

        function validateOtp() {

            let regnum = new RegExp("^[0-9 ]+$");

            let otpValue = $('#otp').val();

            if (otpValue.length == '') {

                $('#otp').css('border-color', '#c70d0d')
                $("#otperror").text('Enter otp')

                otpError = false;
                return false;
            } else if (!regnum.test(otpValue)) {

                $('#otp').css('border-color', '#c70d0d')
                $("#otperror").text('Enter Numbers Only')

                otpError = false;
            } else if (otpValue.length != 6) {

                $('#otp').css('border-color', '#c70d0d')
                $("#otperror").text('OTP is 6 digits')

                otpError = false;
            }

        }
        validateOtp()
        if(otpError===true){
            return true
        }else{
            return false
        }

        }
    </script>



