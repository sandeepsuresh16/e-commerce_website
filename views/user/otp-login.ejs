                    <a href="/login">Login with email & password</a>
                </div>
            </form>
            <%}%>
            <%if(showOTP){%>
                <form action="/verify-otp" method="post" onsubmit="return validateForm1()">
                    <div class="mb-3 mt-3 pl-5 pr-5">
                        <input type="tel" class="from-labrl" style="width: 100%;" value="<%=number%>" name="phone"><br>
                        <label for="otp" class="form-label">Enter OTP</label>
                        <input type="tel" class="form-control" id="otp" style="width: 100%;" placeholder="- - - - - -" name="otp">
                        <p id="otperror" style="color: red;"></p>
                    </div>
