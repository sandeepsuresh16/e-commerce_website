                <h5>OTP-LOGIN</h5>
                <h4 class="text-danger"><%=status%></h4>
            </div>
            <%if(!showOTP){%>
            <form action="/get-otp" method="post" onsubmit="return validateForm()">
                <div class="mb-3 mt-3 pl-5">
                    <label for="phone" class="form-label">Phone Number</label>
                    <input type="tel" class="form-control" id="phone" placeholder="mobile number" name="phone">
